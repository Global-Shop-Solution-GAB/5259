F.ODBC.Connection!con.Close

F.Intrinsic.Control.CallSub(LoadAutoExportColumns, "Columns", V.Local.sColumns)

F.Intrinsic.Control.If(V.Local.sSplit, !=, "NO")
	F.Intrinsic.String.Replace(V.Local.sSplit, "DT", "DEPT", V.Local.sSplit)
	F.Data.DataView.Create("dtWC", "dvDistinct", 22)
	F.Data.DataView.ToDataTableDistinct("dtWC", "dvDistinct", "dtDistinct", V.Local.sSplit)
	F.Data.DataView.Close("dtWC", "dvDistinct")
	
	F.Intrinsic.Control.For(V.Local.iCounter, 0, V.DataTable.dtDistinct.RowCount--, 1)
		F.Intrinsic.String.Build("[{0}] = '{1}' and [OPERATION] <> 'SETUP' and [SHOW] = 'True'", V.Local.sSplit, V.DataTable.dtDistinct(V.Local.iCounter).[V.Local.sSplit].FieldValTrim, V.Local.sFilter)
