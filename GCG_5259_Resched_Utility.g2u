Program.Sub.LoadViewDropDown.Start
'load the user's view, and the others if they are an admin

F.Intrinsic.Control.Try

V.Local.sGroups.Declare(String)
V.Local.iCounter.Declare(String)
V.Local.sSQL.Declare(String)
V.Local.sRet.Declare(String)
V.Local.bAdmin.Declare(Boolean, False)

F.Global.Security.GetUserGroups(V.Caller.User, V.Caller.CompanyCode, V.Local.sGroups)

'should only be one group valid for the user views
F.Intrinsic.String.Replace(V.Local.sGroups, "*!*", "','", V.Local.sGroups)
F.Intrinsic.String.Build("select ADMIN, GS_GROUP from GCG_5259_USER_VIEWS where GS_GROUP in ('{0}') order by GS_GROUP", V.Local.sGroups, V.Local.sSQL)

F.Data.DataTable.CreateFromSQL("dtGroups", "Con", V.Local.sSQL)
F.Intrinsic.Control.If(V.DataTable.dtGroups.RowCount, >, 0)
	F.Data.DataView.Create("dtGroups", "dvViewCheck", 22, "ADMIN = True", "")
	F.Intrinsic.Control.If(V.DataView.dtGroups!dvViewCheck.RowCount, >, 0)
		GUI.frmResched.lblView.Visible(True)
		GUI.frmResched.ddlView.Visible(True)
		V.Local.bAdmin.Set(True)
	F.Intrinsic.Control.EndIf
	F.Data.DataView.Close("dtGroups", "dvViewCheck")
	
	F.Intrinsic.Control.If(V.Local.bAdmin)
		'admin user, get all groups
		F.Data.DataTable.Close("dtGroups")
		F.Data.DataTable.CreateFromSQL("dtGroups", "Con","select RTRIM(GS_GROUP) as GS_GROUP from GCG_5259_USER_VIEWS order by GS_GROUP ")
		GUI.frmResched.ddlView.ClearItems
		'GUI.frmResched.ddlView.AddItem("")
		F.Data.DataView.Create("dtGroups", "dvGroup", 22, "GS_GROUP like '%INX'", "")
		GUI.frmResched.ddlView.AddItems("DataView", "dtGroups", "dvGroup", "GS_GROUP", "GS_GROUP")
		F.Data.DataView.SetFilter("dtGroups", "dvGroup", "GS_GROUP not like '%INX'")
		GUI.frmResched.ddlView.AddItems("DataView", "dtGroups", "dvGroup", "GS_GROUP", "GS_GROUP")
		F.Data.DataView.Close("dtGroups", "dvGroup")
	F.Intrinsic.Control.Else
		GUI.frmResched.ddlView.AddItems("DataTable", "dtGroups", "GS_GROUP", "GS_GROUP")
	F.Intrinsic.Control.EndIf
	F.Data.DataTable.Close("dtGroups")
	
	F.Intrinsic.Control.BlockEvents
	GUI.frmResched.ddlView.ListIndex(0)
	F.Intrinsic.Control.UnBlockEvents
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.If(V.DataTable.dtGroups.Exists)
	F.Data.DataTable.Close("dtGroups")
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.Catch
	F.Intrinsic.Control.CallSub(ErrorMessage, "ErrorNumber", V.Ambient.ErrorNumber, "ErrorDescription", V.Ambient.ErrorDescription, "Subroutine", V.Ambient.CurrentSubroutine, "UNATTENDED", V.Global.bUnattended, "Project", "5259", "ProjectTitle", "5259 Movable Dispatch List Auto Export", "Callback", "frmResched_UnLoad")
F.Intrinsic.Control.EndTry
Program.Sub.LoadViewDropDown.End

