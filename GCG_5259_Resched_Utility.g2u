Program.Sub.GetPrevWC.Start
'This subroutine returns the previous WC for a previous WO Seq where the OP Code equals to RUNTIM

F.Intrinsic.Control.Try

	V.Local.iCounter.Declare(Long)
	V.Local.sSQL.Declare(String)
	V.Local.sWC.Declare(String)
	
	'Creating dv, SHOULD BE WIP
	F.Data.DataView.Create("dtWC", "dvWC", 22,"[OPERATION] = 'WIP'", "")	
	
	'Looping through dv to add the lines for the PO that will be generated
	F.Intrinsic.Control.For(V.Local.iCounter, 0, V.DataView.dtWC!dvWC.RowCount--, 1) 
		
		'Getting WC from previous seq and setting value in dv
		F.Intrinsic.String.Build("select RTRIM(PART) WORKCENTER from V_JOB_OPERATIONS where JOB = '{0}' and SUFFIX = '{1}' and OPERATION = 'RUNTIM' order by SEQ desc limit 1;", V.DataView.dtWC!dvWC(V.Local.iCounter).JOB!FieldValTrim, V.DataView.dtWC!dvWC(V.Local.iCounter).SFX!FieldValTrim, V.Local.sSQL)
		F.ODBC.Connection!Con.ExecuteAndReturn(V.Local.sSQL, V.Local.sWC)
		F.Data.DataView.SetValue("dtWC", "dvWC", V.Local.iCounter, "WM", V.Local.sWC)
		
	F.Intrinsic.Control.Next(V.Local.iCounter)
	F.Data.DataView.Close("dtWC", "dvWC")

F.Intrinsic.Control.Catch
	F.Intrinsic.Control.CallSub(ErrorMessage, "ErrorNumber", V.Ambient.ErrorNumber, "ErrorDescription", V.Ambient.ErrorDescription, "Subroutine", V.Ambient.CurrentSubroutine, "UNATTENDED", V.Global.bUnattended, "Project", "5259", "ProjectTitle", "5259 Movable Dispatch List Auto Export", "Callback", "frmResched_UnLoad")
F.Intrinsic.Control.EndTry
Program.Sub.GetPrevWC.End

