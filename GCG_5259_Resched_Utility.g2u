		F.Intrinsic.String.Build("5259_Schedule_{0}", V.DataTable.dtDistinct(V.Local.iCounter).[V.Local.sSplit].FieldValTrim, V.Local.sFileName)
		
		F.Intrinsic.Control.CallSub(AutoExport, "Form", "frmResched", "GridControl", "GsGCWC", "GridView", "gvWC", "FileName", V.Local.sFileName, "FilePath", V.Local.sPath, "TYPE", "xlsx")
		F.Intrinsic.Control.CallSub(AutoExport, "Form", "frmResched", "GridControl", "GsGCWC", "GridView", "gvWC", "FileName", V.Local.sFileName, "FilePath", V.Local.sPath, "TYPE", "html")
	F.Intrinsic.Control.Next(V.Local.iCounter)
F.Intrinsic.Control.Else
	F.Intrinsic.Control.CallSub(AutoExport, "Form", "frmResched", "GridControl", "GsGCWC", "GridView", "gvWC", "FileName", "5259_Schedule", "FilePath", V.Local.sPath)
